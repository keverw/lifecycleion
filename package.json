{
  "name": "lifecycleion",
  "version": "0.0.1",
  "type": "module",
  "description": "",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run update-readme && tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "update-readme": "markdown-toc-gen update README.md && bun run scripts/update-readme-version.ts"
  },
  "keywords": [],
  "author": "Kevin Whitman (https://github.com/keverw)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/keverw/lifecycleion"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "markdown-toc-gen": "^1.2.0",
    "@types/bun": "^1.2.13",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "prettier": "^3.6.2",
    "tsup": "^8.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  },
  "dependencies": {}
}
